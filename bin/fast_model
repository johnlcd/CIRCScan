#!/bin/bash

echo ======================================

echo " "
echo USAGE:
echo " "
echo " $0 -t <cell type> -m <model> -n <cores> "
echo " "

echo --------------------------------------

echo " "
echo EXAMPLEs:
echo " "
echo " $0 -t GM12878 -m rf -n 10 "
echo " "

echo ======================================

echo " "
echo Command is: $0 $*
echo " "


if [ $# == "6" ] && [ $1 == "-t" ] && [ $3 == "-m" ] && [ $5 == "-n" ]; then


	nohup R CMD BATCH --slave "--args $2 $4 $6" $PKG_DIR/bin/model/fast_model.R $2_$4_fast_model.out


fi

